  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "chromeTest": "npm run test -- --browser chrome",
    "GitReport": "npm run test -- --record --key c1495532-9f10-45b6-be55-b5df9247db93",
    "SyncupSute": "npm run test -- --spec \"cypress/integration/SyncUP/*\"",
    "Dashboard": "npm run test -- --spec \"cypress/integration/Trualta/Caregiver.spec.js\" --headed  --record --key c1495532-9f10-45b6-be55-b5df9247db93 ",
    "Caregiver": "npm run test -- --spec \"cypress/integration/Trualta/Caregiver.spec.js\" --browser chrome --reporter mochawesome"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
